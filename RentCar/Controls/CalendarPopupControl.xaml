<UserControl x:Class="RentCar.Controls.CalendarPopupControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RentCar.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Loaded="UserControl_Loaded">
    <Grid Height="170" Width="200" Background="#00000000">
        <Calendar  HorizontalAlignment="Stretch" Margin="0" Name="calendarControl" VerticalAlignment="Top" HorizontalContentAlignment="Stretch" />
    </Grid>
</UserControl>
